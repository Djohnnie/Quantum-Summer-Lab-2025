    <MudDrawer Open="true" Anchor="Anchor.End" ClipMode="DrawerClipMode.Always" Variant="DrawerVariant.Persistent" Width="25%" Elevation="0">
        <MudText Class="mt-12 mb-2" Typo="Typo.button">Contents</MudText>
        <MudNavMenu Class="nav-border" Bordered="true" Color="Color.Default">
            <MudNavLink OnClick="@((e) => OnNavigateToHeading("#prerequisites"))">Prerequisites</MudNavLink>
